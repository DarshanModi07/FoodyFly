{
  "name": "foodyfly",
  "version": "1.0.0",
  "description": "A modern food ordering and restaurant discovery platform built with React, Redux, and Tailwind CSS",
  "author": {
    "name": "Darshan",
    "url": "https://github.com/darshan-tech"
  },
  "license": "MIT",
  "homepage": "https://github.com/darshan-tech/foodyfly",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/darshan-tech/foodyfly.git"
  },
  "bugs": {
    "url": "https://github.com/darshan-tech/foodyfly/issues"
  },
  "keywords": [
    "react",
    "food-ordering",
    "restaurant",
    "redux",
    "tailwind-css",
    "react-router",
    "parcel"
  ],
  "scripts": {
    "start": "parcel index.html",
    "test": "jest --config config/jest.config.js",
    "test:coverage": "jest --coverage --config config/jest.config.js",
    "test:watch": "jest --watch --config config/jest.config.js",
    "build": "parcel build index.html",
    "clean": "rm -rf build .parcel-cache dist",
    "lint": "echo 'No linter configured yet'",
    "prestart": "npm run clean"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "babel-jest": "^30.2.0",
    "global-jsdom": "^27.0.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jsdom": "^27.3.0",
    "parcel": "^2.16.0"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.11.1",
    "@tailwindcss/postcss": "^4.1.17",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-redux": "^9.2.0",
    "react-router": "^6.28.0",
    "react-router-dom": "^6.28.0",
    "tailwindcss": "^4.1.17"
  },
  "targets": {
    "default": {
      "distDir": "build"
    }
  }
}
